                        <location>view</location>
                    </source-folder>
                    <source-folder style="tree">
                        <label>resources</label>
                        <location>resources</location>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                </items>
                <context-menu>
